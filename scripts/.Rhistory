library(lmerTest);library(multcomp);library("ggplot2")
fit <- lmer(atention ~ awareness_1 + awareness_2 + Awareness_3 + awareness_4 + confidence_1 + confidence_2 + confidence_3 + confidence_4 + accuracy_1 + accuracy_2 + accuracy_3 + accuracy_4 + (1|sub_name),data=DataSelect)
